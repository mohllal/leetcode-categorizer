# LeetCode session token (required for authenticating API requests)
LEETCODE_SESSION=leetcode_session

# Name of the output markdown file (default: OUTPUT.md)
OUTPUT_FILE=OUTPUT.md

# Enable or disable the solution linking feature (true to enable, false to disable)
ENABLE_SOLUTION_LINKS=true

# Path to the directory containing local solutions
SOLUTIONS_DIR=/path/to/your/solutions